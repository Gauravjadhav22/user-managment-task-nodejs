{
  "name": "user-management-task-backend",
  "version": "1.0.0",
  "main": "index.js",
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "scripts": {
    "start": "dotenv -e .env.development -- node dist/index.js",
    "dev": "dotenv -e .env.development -- nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "seed": "ts-node prisma/seed.ts",
    "migrate": "dotenv -e .env.development -- npx prisma migrate dev && dotenv -e .env.development -- npx prisma generate",
    "migrate:deploy": "dotenv -e .env.production -- npx prisma migrate deploy && dotenv -e .env.production -- npx prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4",
    "typescript": "^5.6.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.7",
    "cors": "^2.8.5",
    "nodemon": "^3.1.7",
    "prisma": "^5.21.1",
    "ts-node": "^10.9.2"
  }
}
